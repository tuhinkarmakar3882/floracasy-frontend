{
  "name": "floracasy",
  "appName": "Floracasy",
  "description": "The One Stop Hub for Passionate & Creative minds",
  "version": "1.0.4",
  "themeColor": "#0d0d17",
  "private": true,
  "resolutions": {
    "prosemirror-model": "1.9.1"
  },
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts build --modern && nuxt-ts start --modern",
    "generate": "nuxt-ts generate",
    "pm2": "nuxt-ts build && pm2 start ecosystem.config.js",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:style-scss": "stylelint '**/*.scss' --config ./stylelint.config.js --fix",
    "lint:style-vue": "stylelint '**/*.vue' --config ./stylelint.config.js --fix",
    "lint": "yarn lint:js && yarn lint:style-vue && yarn lint:style-scss",
    "test": "snyk test && jest"
  },
  "dependencies": {
    "@mdi/font": "^5.8.55",
    "@nuxt/content": "^1.8.1",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/markdownit": "^1.2.10",
    "@nuxtjs/pwa": "^3.0.2",
    "browser-image-compression": "^1.0.13",
    "chart.js": "^2.9.4",
    "cookie-universal-nuxt": "^2.1.4",
    "cookieparser": "^0.1.0",
    "core-js": "^3.6.5",
    "dayjs": "^1.9.8",
    "express": "^4.17.1",
    "firebase": "^8.0.0",
    "firebase-tools": "9.0.0",
    "highlight.js": "^10.3.2",
    "image-url-validator": "^0.2.4",
    "js-cookie": "^2.2.0",
    "moment": "^2.29.1",
    "node-sass": "^4.14.1",
    "nuxt": "^2.14.5",
    "nuxt-compress": "^4.1.0",
    "nuxt-helmet": "^2.0.0",
    "nuxt-lazy-load": "^1.2.4",
    "reconnecting-websocket": "^4.4.0",
    "sanitize-html": "^2.1.1",
    "sass-loader": "^10.0.3",
    "tiptap": "^1.30.0",
    "tiptap-extensions": "^1.33.2",
    "vue-infinite-loading": "^2.4.5",
    "vue-quill-editor": "^3.0.6",
    "vue2-dropzone": "^3.6.0",
    "vue2-touch-events": "^3.0.0",
    "vuetrend": "^0.3.4",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/content": "^1.8.1",
    "@nuxt/types": "^2.14.5",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/eslint-config": "^4.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/pwa": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^3.0.2",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "core-js": "^3.6.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "nuxt": "^2.14.5",
    "nuxt-purgecss": "^1.0.0",
    "nuxt-twa-module": "^1.3.1",
    "prettier": "^2.1.1",
    "snyk": "^1.434.2",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.3.0",
    "vue": "^2.6.12",
    "vue-client-only": "^2.0.0",
    "vue-jest": "^3.0.4"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint",
    "*.{scss,vue}": "stylelint"
  }
}
